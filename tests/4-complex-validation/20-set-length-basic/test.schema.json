{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "unique_values": {
      "type": "array",
      "description": "A set of unique, non-empty strings (4-7 items).",
      "items": {
        "type": "string"
      },
      "minItems": 4,
      "maxItems": 7,
      "uniqueItems": true
    }
  },
  "required": [
    "unique_values"
  ],
  "additionalProperties": true
}
